{
  "name": "people-server",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/main/code/app.js",
    "test": "jest --detectOpenHandles ./build/test",
    "watch:dev": "concurrently \"tsc -w\" \"nodemon -w ./build/main/code/app.js\"",
    "build:test": "npm run build && npm run test"
  },
  "engines": {
    "node": "16"
  },
  "main": "./build/main/code/app.js",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "typescript": "^4.5.5"
  },
  "private": true
}
